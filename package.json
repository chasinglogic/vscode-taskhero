{
    "name": "tsk",
    "displayName": "tsk",
    "description": "",
    "version": "0.0.2",
    "publisher": "tsk",
    "repository": {
        "url": "https://github.com/chasinglogic/vscode-tsk",
        "type": "git"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.completeTask",
                "title": "Tsk: Complete Task"
            },
            {
                "command": "extension.completeTaskById",
                "title": "Tsk: Complete Task By ID"
            },
            {
                "command": "extension.newTask",
                "title": "Tsk: New Task"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
